                $container.masonry({
                    columnWidth: '.item',
                    itemSelector: '.item'
                });
            });
            // 当内容是隐藏时，进入不会自动布局，所以要重新调用上面的代码。
            $('a[data-toggle=tab]').each(function() {
                var $this = $(this);
                $this.on('shown.bs.tab', function() {
                    $container.imagesLoaded(function() {
                        $container.masonry({
                            columnWidth: '.item',
                            itemSelector: '.item'
                        });
                    });
                }); //end shown
            }); //end each
        </script>

    </div>

